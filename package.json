{
  "name": "jump-to-tests",
  "displayName": "Jump to Tests",
  "description": "Switch between the code and the test file",
  "version": "0.0.1",
  "publisher": "schmas",
  "engines": {
    "vscode": "^1.44.0"
  },
  "repository": {
    "url": "https://github.com/schmas/vscode-jump-to-tests"
  },
  "icon": "images/jump-to-tests.png",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jump-to-tests.jump2"
  ],
  "main": "./out/main/extension.js",
  "contributes": {
    "configuration": {
      "title": "Jump to Tests",
      "properties": {
        "jump-to-tests.rules": {
          "type": "array",
          "description": "Additional {pattern, replacement} rules - see extension description",
          "default": []
        }
      }
    },
    "commands": [
      {
        "command": "jump-to-tests.jump2",
        "title": "Jump to Tests: Jump2"
      }
    ],
    "keybindings": [
      {
        "command": "jump-to-tests.jump2",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run lint && npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint '*/**/*.{js,ts}' --fix",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "get-vscode-version": "echo $npm_package_engines_vscode"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/lodash": "^4.14.150",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "@types/vscode": "^1.44.0",
    "@typescript-eslint/eslint-plugin": "^2.26.0",
    "@typescript-eslint/parser": "^2.26.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "glob": "^7.1.6",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.7",
    "mocha": "^7.1.1",
    "prettier": "^2.0.4",
    "typescript": "^3.8.3",
    "vscode-test": "^1.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "tsc --noEmit",
      "eslint --cache --fix"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.15"
  }
}
